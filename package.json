{
  "name": "@a11ywatch/mav",
  "version": "0.5.1",
  "description": "a gRPC image recognition service",
  "main": "./server.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "pub": "tsc && tsc-alias && cp package.json dist && cp package-lock.json dist && cp README.md dist && cp LICENSE dist && cd dist && npm publish"
  },
  "dependencies": {
    "@a11ywatch/protos": "^0.3.3",
    "@azure/cognitiveservices-computervision": "^8.2.0",
    "@grpc/grpc-js": "1.6.12",
    "@grpc/proto-loader": "0.7.2",
    "@tensorflow-models/coco-ssd": "2.2.2",
    "@tensorflow-models/mobilenet": "2.1.0",
    "@tensorflow/tfjs-backend-wasm": "3.20.0",
    "@tensorflow/tfjs-converter": "3.20.0",
    "@tensorflow/tfjs-core": "3.20.0",
    "base64-to-tensor": "^1.2.8",
    "tensornet": "1.0.14",
    "jpeg-js": "^0.4.4"
  },
  "devDependencies": {
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^28.1.1",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "ts-node-dev": "1.1.6",
    "tsc-alias": "1.6.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
